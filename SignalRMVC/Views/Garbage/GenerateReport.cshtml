
@{
    ViewBag.Title = "Reports";
}

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title"> Distibution of the data types in the file </h3>
    </div>
    <div class="panel-body">
        <div class="row col-md-12">
            <table class="table table-bordered table-hover table-striped">
                <thead>
                    <tr>
                        <th scope="col">% Numeric</th>
                        <th scope="col">% AlphaNumeric</th>
                        <th scope="col">% Float</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@ViewBag.Numeric</td>
                        <td>@ViewBag.AlphaNumeric</td>
                        <td>@ViewBag.Float</td>
                    </tr>
                </tbody>
            </table>
       </div>
        <div class="row text-center">
            <hr />
            <span><a href="/" class="btn btn-primary" id="back"> <i class="glyphicon glyphicon-backward"></i> &nbsp;Back</a>  &nbsp;</span>
        </div>

    </div>

</div>
